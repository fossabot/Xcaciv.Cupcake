<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<packageSources>
		<clear />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
		<add key="github" value="https://nuget.pkg.github.com/xcaciv/index.json" />
		<add key="local" value="G:\NuGetPackages" />

		<add key="Command" value="https://github.com/Xcaciv/Xcaciv.Command" />

		<add key="Loader" value="https://github.com/Xcaciv/Xcaciv.Loader" />

	</packageSources>

	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>

		<packageSource key="local">
			<package pattern="Xcaciv.*" />
		</packageSource>
	</packageSourceMapping>

</configuration>